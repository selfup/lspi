{
  "name": "lspi",
  "version": "3.0.3",
  "description": "A high level API/ORM for localStorage",
  "main": "dist/lspi.js",
  "jsnext:main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/selfup/lspi"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "mkdir dist; npm run build && jest --coverage --no-cache",
    "build": "npm run bundle && npm run minify",
    "bundle": "rollup -c",
    "minify": "uglifyjs dist/lspi.js -o dist/lspi.js",
    "prepare": "npm run build",
    "release": "npm run build && npm test && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "babel": {
    "presets": "env"
  },
  "jest": {
    "transform": {}
  },
  "keywords": [
    "local",
    "storage",
    "local storage",
    "localStorage",
    "api",
    "client",
    "clienside api",
    "client side",
    "client side",
    "client side active record",
    "orm",
    "local storage orm",
    "local storage database management",
    "local storage api"
  ],
  "author": "selfup - Regis Jean-Pierre Boudinot",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^24.8.0",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "uglify-js": "3.2.2"
  }
}
