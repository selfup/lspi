{
  "name": "lspi",
  "version": "3.1.2",
  "description": "A high level API/ORM for localStorage",
  "main": "dist/lspi.js",
  "jsnext:main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/selfup/lspi"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "npm run build && jest --coverage --no-cache",
    "build": "npm run bundle && npm run minify",
